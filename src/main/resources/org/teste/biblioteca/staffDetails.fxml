<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.teste.biblioteca.StaffDetails">
   <children>
      <TableView fx:id="myTableView" layoutX="-2.0" prefHeight="340.0" prefWidth="600.0">
        <columns>
          <TableColumn fx:id="idColumn" prefWidth="200.0" style="-fx-font-family: Roboto;" text="ID" />
          <TableColumn fx:id="nameColumn" prefWidth="200.0" style="-fx-font-family: Roboto;" text="Nome" />
            <TableColumn fx:id="contactColumn" prefWidth="200.0" style="-fx-font-family: Roboto;" text="Contato" />
        </columns>
      </TableView>
      <Button fx:id="refreshButton" layoutX="159.0" layoutY="354.0" mnemonicParsing="false" onAction="#refreshButton" prefHeight="32.0" prefWidth="95.0" text="Atualizar" />
      <Button fx:id="backButton" layoutX="343.0" layoutY="354.0" mnemonicParsing="false" onAction="#backButton" prefHeight="32.0" prefWidth="95.0" text="Voltar" />
   </children>
</AnchorPane>
